<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#aa9e45">

        <androidx.core.widget.NestedScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fillViewport="true"
            android:scrollbars="none"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.google.android.material.card.MaterialCardView
                    style="@style/CardViewDashboard"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_margin="@dimen/_7sdp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_40sdp"
                            tools:ignore="VectorDrawableCompat"
                            android:layout_marginTop="@dimen/_10sdp"
                            app:srcCompat="@drawable/plot"/>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Plot"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Booking Plot"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />

                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="@dimen/_30sdp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/googlesans_regular"
                                    android:text="0"
                                    android:textColor="@color/black"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                    android:textSize="@dimen/_12sdp" />


                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">


                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Available Plot"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />

                        </androidx.appcompat.widget.LinearLayoutCompat>
                        </androidx.appcompat.widget.LinearLayoutCompat>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/CardViewDashboard"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_margin="@dimen/_7sdp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_40sdp"
                            tools:ignore="VectorDrawableCompat"
                            android:layout_marginTop="@dimen/_10sdp"
                            app:srcCompat="@drawable/member"/>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Member"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Active"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />
                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">


                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="IN Active"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />
                                <androidx.appcompat.widget.AppCompatTextView
                                    android:layout_width="@dimen/_30sdp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/googlesans_regular"
                                    android:text="0"
                                    android:textColor="@color/black"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                    android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>


                            </androidx.appcompat.widget.LinearLayoutCompat>
                        </androidx.appcompat.widget.LinearLayoutCompat>




                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/CardViewDashboard"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_margin="@dimen/_7sdp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_40sdp"
                            tools:ignore="VectorDrawableCompat"
                            android:layout_marginTop="@dimen/_10sdp"
                            app:srcCompat="@drawable/directmember"/>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Direct Member"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Direct"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>


                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="IN Direct"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_10sdp" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>
                        </androidx.appcompat.widget.LinearLayoutCompat>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    style="@style/CardViewDashboard"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_70sdp"
                    android:layout_margin="@dimen/_7sdp"
                    app:cardBackgroundColor="@color/white">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_40sdp"
                            tools:ignore="VectorDrawableCompat"
                            android:layout_marginTop="@dimen/_10sdp"
                            app:srcCompat="@drawable/business"/>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Business"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Direct"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_9sdp" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="?attr/selectableItemBackground">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_200sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="Matching"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_9sdp" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="@dimen/_30sdp"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/googlesans_regular"
                                android:text="0"
                                android:textColor="@color/black"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Menu"
                                android:textSize="@dimen/_12sdp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>
                        </androidx.appcompat.widget.LinearLayoutCompat>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>


            </androidx.appcompat.widget.LinearLayoutCompat>


        </androidx.core.widget.NestedScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
